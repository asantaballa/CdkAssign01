@model CdkAssign01.Models.OrdersListViewModel

@{
    ViewBag.Title = "Lookup Page";
}

<div class="row">
    <br />
    <table>
        <thead>
            <tr>
                <th>Customer</th>
                <th>Order</th>
                <th>First</th>
                <th>Last</th>
                <th>City</th>
                <th>State</th>
                <th>Zip</th>
            </tr>
        </thead>
        @foreach (var order in Model.Orders)
            {
            <tr>
                <td>@order.CustomerId</td>
                <td>@order.OrderId</td>
                <td>@order.FirstName</td>
                <td>@order.LastName</td>
                <td>@order.City</td>
                <td>@order.StateCode</td>
                <td>@order.PostalCode</td>

            </tr>
        }
    </table>
 
</div>
